{
  "expo": {
    "name": "FarmFerry User",
    "slug": "farmferry-user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/OutlookLogo2.png",
    "scheme": "farmferry-user",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/your-project-id"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farmferry.user"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/OutlookLogo2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.farmferry.user",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/OutlookLogo2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "54c01b7e-6c33-4679-b0b3-7e7c2057fa7c"
      }
    }
  }
}
