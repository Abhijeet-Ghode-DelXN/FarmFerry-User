{
  "expo": {
    "name": "FarmFerry User",
    "slug": "farmferry-user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/OutlookLogo2.png",
    "scheme": "farmferry-user",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/your-project-id"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farmferry.user"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/OutlookLogo2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.farmferry.user",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBXdP3XIM-0uKS77yHw_YaZpRUSMTKc9xw"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/OutlookLogo2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app uses location to show delivery addresses."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e41fc391-bd39-482c-b3db-54e9a4ddcca9"
      }
    }
  }
}